  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <a class="nav-link" href="{{path('app_employe')}}">Employés</a>
      </li>
            <li class="nav-item">
        <a class="nav-link" href="{{path('app_entreprise')}}">Entreprises</a>
